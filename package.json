{
  "name": "login-test-automation",
  "version": "1.0.0",
  "description": "Test automation for SauceDemo login using Cypress and Selenium",
  "main": "index.js",
  "scripts": {
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "selenium:test": "mocha selenium/tests/*.test.js --timeout 30000",
    "install:selenium": "npm install selenium-webdriver mocha chai --save",
    "install:cypress": "npm install cypress@13.6.0 --save-dev"
  },
  "keywords": [
    "cypress",
    "selenium",
    "test-automation"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "selenium-webdriver": "^4.38.0"
  },
  "devDependencies": {
    "chai": "^4.5.0",
    "cypress": "^13.6.0",
    "mocha": "^10.8.2"
  }
}
